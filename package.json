{
  "name": "elm-electron-webpack",
  "version": "1.0.0",
  "description": "Some learnings on using Elm with Electron and Webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run wp-watch\" \"npm run e-launch\"",
    "wp-watch": "webpack-dev-server --content-base /dist",
    "e-launch": "electron main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "elm"
  ],
  "author": "micah",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.4.14",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "concurrently": "^3.1.0",
    "elm-webpack-loader": "^4.2.0"
  }
}
